{
  "name": "categorise_frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "generate-api-types": "openapi-generator-cli generate -i http://localhost:8000/openapi.json -g typescript-axios -o src/types/generated",
    "format": "prettier --write src/",
    "analyse-bundle": "vite-bundle-visualizer"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@tanstack/vue-query": "^5.37.1",
    "axios": "^1.7.2",
    "convert": "^5.2.0",
    "pinia": "^2.1.7",
    "uuid": "^9.0.1",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "wavesurfer.js": "^7.7.15"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "@vue/typescript-plugin": "^2.0.19",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.5",
    "vite": "^5.2.11",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19"
  }
}
