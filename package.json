{
  "name": "categorise_frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --fix",
    "generate-api-types": "openapi-generator-cli generate -i http://localhost:8000/openapi.json -g typescript-axios -o src/types/generated",
    "format": "prettier --write src/",
    "analyse-bundle": "vite-bundle-visualizer"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@tanstack/vue-query": "^5.62.8",
    "axios": "^1.7.9",
    "convert": "^5.7.0",
    "pinia": "^2.3.0",
    "uuid": "^11.0.3",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5",
    "vue-router": "^4.5.0",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "wavesurfer.js": "^7.8.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@vue/typescript-plugin": "^2.1.10",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.5",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  }
}
